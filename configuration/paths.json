      "source": "configuration/cloudformation.json",
      "build": ".build/configuration/cloudformation.json",
      "remote": "s3://{{ parameters.bucket }}/configuration/cloudformation.json"
    }
  },
  "ec2": {
    "discord_bot": {
      "source": "src/discord-bot/",
      "build": ".build/discord-bot/",
